countries:
- US
alerts:
  drop_threshold: 5
  rise_threshold: 10
  smart_alerts:
    enabled: true
    pattern_detection: true
    contextual_insights: true
  daily_summary:
    enabled: true
    time: '18:00'
    min_changes: 3
    include_priorities:
    - MEDIUM
    - LOW
  telegram:
    enabled: true
    bot_token: YOUR_BOT_TOKEN_HERE
    chat_id: YOUR_CHAT_ID_HERE
  slack:
    enabled: false
    webhook_url: ''
schedule:
  daily_check_time: '16:00'
  post_update_checks:
  - 1
  - 6
  - 24
  - 72
google_calendar:
  enabled: false
  credentials_file: config/credentials.json
  token_file: config/token.json
  calendar_id: primary
  update_keywords:
  - app update
  - app-update
  - nueva version
  - update biblenow
trends:
  google_trends:
    enabled: false
    region: US
  ai_analysis:
    enabled: false
    api_key: ''
    model: gpt-4o-mini
pro_features:
  competitor_tracking:
    enabled: true
    track_top_n: 5
    update_frequency: daily
  keyword_discovery:
    enabled: true
    auto_discover: true
    sources:
    - apple_suggest
    - competitors
    - long_tail
    min_opportunity_score: 50
  ab_testing:
    enabled: true
    auto_detect_changes: true
  seasonal_analysis:
    enabled: true
    min_history_days: 14
    predict_movements: true
  cost_analysis:
    enabled: true
    business_metrics:
      avg_cvr: 0.03
      avg_retention_d1: 0.4
      avg_retention_d7: 0.15
      avg_arpu_monthly: 2.5
      avg_ltv_6months: 12.0
  dashboard:
    enabled: true
    auto_refresh: true
    refresh_interval_minutes: 60
    include_all_features: true
storage:
  ranks_file: data/ranks.csv
  log_file: logs/rank_guard.log
  retention_days: 90
api:
  itunes:
    base_url: https://itunes.apple.com/search
    limit: 250
    timeout: 10
  rate_limit:
    requests_per_minute: 20
    delay_between_requests: 3
debug:
  enabled: false
  verbose_logs: false
  test_mode: false
